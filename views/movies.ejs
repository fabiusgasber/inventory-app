<%- include("components/head") %>
<body>
    <header>
        <h1><%= title %></h1>
        <%- include("components/navbar") %>
    </header>
    <main>
        <% movies.forEach((movie) => { %>
            <article>
                <a href="/movie/<%= movie.id %>"><img src="<%= movie.src %>"></a>
                <h2><%= movie.title %></h2>
                <p><%= movie.rating %></p>
                <p><%= movie.price %></p>
                <p><%= movie.quantity %></p>
                <div>
                    <a href="/movie/<%= movie.id %>/edit">Edit</a>
                    <button type="button" onclick="deleteFn('<%= movie.id %>')">Delete</button>
                </div>
            </article>
        <% }) %>
    </main>
</body>
</html>